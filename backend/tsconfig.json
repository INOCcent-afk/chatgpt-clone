{
    "compilerOptions": {
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "target": "ES2020",
      "sourceMap": true,
      "outDir": "dist",
      "strictNullChecks": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
    },
    "include": ["src/**/*"],
  }